{
  "compilerOptions": {
    "verbatimModuleSyntax": true,
    "paths": {
      "@/*": ["./src/*"]
    },
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "types": ["astro/client"]
  },
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"]
}
